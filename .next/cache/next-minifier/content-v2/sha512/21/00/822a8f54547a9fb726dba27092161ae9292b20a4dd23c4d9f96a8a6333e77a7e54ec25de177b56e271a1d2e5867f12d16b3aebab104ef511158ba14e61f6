/index.js","/home/apollo/Downloads/seandre_backup/node_modules/next/dist/build/webpack/loaders/next-babel-loader.js??ref--6-0!/home/apollo/Downloads/seandre_backup/node_modules/file-rawtxt-loader/index.js!/home/apollo/Downloads/seandre_backup/public/txt/journal/210125__We_Gon_Be_All_Right.txt","/home/apollo/Downloads/seandre_backup/node_modules/next/dist/build/webpack/loaders/next-babel-loader.js??ref--6-0!/home/apollo/Downloads/seandre_backup/node_modules/file-rawtxt-loader/index.js!/home/apollo/Downloads/seandre_backup/public/txt/journal/210213__Phrases_That_Annoy_Me.txt","/home/apollo/Downloads/seandre_backup/node_modules/next/dist/build/webpack/loaders/next-babel-loader.js??ref--6-0!/home/apollo/Downloads/seandre_backup/node_modules/file-rawtxt-loader/index.js!/home/apollo/Downloads/seandre_backup/public/txt/journal/201205__My_First_Entry.txt","/home/apollo/Downloads/seandre_backup/node_modules/react-is/index.js","/home/apollo/Downloads/seandre_backup/node_modules/next/link.js","/home/apollo/Downloads/seandre_backup/node_modules/next/dist/build/webpack/loaders/next-babel-loader.js??ref--4!/home/apollo/Downloads/seandre_backup/node_modules/next/dist/client/link.js","/home/apollo/Downloads/seandre_backup/node_modules/@babel/runtime/helpers/esm/inheritsLoose.js","/home/apollo/Downloads/seandre_backup/node_modules/next/dist/compiled/webpack/global.js","/home/apollo/Downloads/seandre_backup/node_modules/react-is/cjs/react-is.production.min.js","/home/apollo/Downloads/seandre_backup/node_modules/mini-create-react-context/dist/esm/index.js","/home/apollo/Downloads/seandre_backup/node_modules/next/dist/build/webpack/loaders/next-babel-loader.js??ref--4!/home/apollo/Downloads/seandre_backup/node_modules/next/dist/client/use-intersection.js","/home/apollo/Downloads/seandre_backup/node_modules/path-to-regexp/index.js","/home/apollo/Downloads/seandre_backup/node_modules/next/dist/build/webpack/loaders/next-babel-loader.js??ref--6-0!/home/apollo/Downloads/seandre_backup/node